        int nr = nz / N;       // Average nonzeros per row.
        int anz = nr * N;      // Actual number of nonzeros.
        double[] val = randomVector(anz, rand2);
        int[] col = new int[anz];
        int[] row = new int[N + 1];
        row[0] = 0;
        for (int r = 0; r < N; r++) {
            int rowr = row[r];
            row[r + 1] = rowr + nr;
            int step = r / nr;
            if (step < 1) step = 1;
            for (int i = 0; i < nr; i++) {
